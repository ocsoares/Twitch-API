{
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/" }],
    "builds": [
        {
            "src": "dist/src/main.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/src/main.js"
        }
    ]
}