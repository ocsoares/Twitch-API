{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/main.ts"
        }
    ],
    "build": {
        "env": {
            "CI": "true"
        },
        "command": "npm install -f && npm run build"
    }
}